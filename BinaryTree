package lab4;
public class BinaryTree<Integer> {

	protected Node<Integer> root;
	public static class Node<Integer>
	{
		Node<Integer> right;
		Node<Integer> left;
		int data; 
		Node()
		{
			data = 0; 
			right = null; 
			left = null;
		}
		Node(int newData)
		{
			data = newData; 
			right = null; 
			left = null;
		}
		Node(int newData, BinaryTree<Integer> rightTree, BinaryTree<Integer> leftTree)
		{
			data = newData;
			if(leftTree != null)
			{
				left = leftTree.root;
			}
			if(rightTree != null)
			{
				right = rightTree.root;
			}
			
		}
		}
		BinaryTree()
		{
			root = null;
		}
		



		public void testing()
		{
		}
	}
